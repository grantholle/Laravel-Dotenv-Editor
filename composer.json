{
    "name": "grantholle/dotenv-editor",
    "description": "The .env file editor tool for Laravel 5+",
    "license": "MIT",
    "keywords": ["laravel", "dotenv", "dotenv-editor"],
    "authors": [
        {
            "name": "Grant Holle",
            "email": "hollegrant@gmail.com"
        }
    ],
    "require": {
        "php": "~7.2",
        "illuminate/config": "~6.0|~7.0",
        "illuminate/container": "~6.0|~7.0",
        "illuminate/support": "~6.0|~7.0"
    },
    "autoload": {
        "psr-4": {
            "GrantHolle\\DotenvEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrantHolle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "GrantHolle\\DotenvEditor\\ServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Barryvdh\\Debugbar\\Facade"
            }
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "orchestra/testbench": "^5.0"
    }
}
